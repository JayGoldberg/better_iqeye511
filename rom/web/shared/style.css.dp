li.zoomout  {background: $V(COLOR) url(zoom.gif) 0 0 no-repeat;}
a.zoomout  {background: $V(COLOR) url(zoom.gif) 100% 66% no-repeat;}
a.zoomout:hover {background: $V(COLOR) url(zoom.gif) 100% 100% no-repeat;}

li.sizep  {background: $V(COLOR) url(size.gif) 0 0 no-repeat;}
