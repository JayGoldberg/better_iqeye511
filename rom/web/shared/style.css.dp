/*  home page specialty buttons, goes with buttonm above */

a.menu:hover {color: $V(HOVERCOLOR);}
li.menu  {background: $V(COLOR) url(buttonm.gif) 0 0 no-repeat;}
a.menu  {background: $V(COLOR) url(buttonm.gif) 100% 0 no-repeat;} 

