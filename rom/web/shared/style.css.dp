a.sizep:hover {background: $V(COLOR) url(size.gif) 100% 33% no-repeat;}

li.sizem  {background: $V(COLOR) url(size.gif) 0 0 no-repeat;}
a.sizem {background: $V(COLOR) url(size.gif) 100% 66% no-repeat;}
a.sizem:hover {background: $V(COLOR) url(size.gif) 100% 100% no-repeat;}

